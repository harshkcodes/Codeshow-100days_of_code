import java.util.*;
class averagearray
{
    public static void main()
    {
        int n,i,c=0;
        double av;
        Scanner sc=new Scanner(System.in);
        System.out.println("enter no. of student");
        n=sc.nextInt();
        int m[]=new int[n];
        for(i=0;i<n;i++)
        {
            m[i]=sc.nextInt();
        }
        for(int j=0;j<n;j++)
        {
         c=c+m[j];
        }
        av=c/n;
        System.out.println("AVERAGE OF ARRAY"+ av);
    }}
        
            
        
