package mathematics;
import java.util.Scanner;
public class trailingzero {
	@SuppressWarnings("resource")
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		int result=0;
		for(int i=5; i<=num; i=i*5) {
			result=result+(num/i);
		}
		System.out.println(result);
	}
}
