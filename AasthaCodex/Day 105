#include<iostream>
using namespace std;
int main(){

    int t;
    int H, M;
    int BMI = 0;
    cin>>t;
    while(t--){

        cin>>H>>M;
        BMI = (H/ (M*M));
        if(BMI <= 18 ){
            cout<<"1"<<endl;
        }
        else if(BMI >= 19 && BMI <= 24){
            cout<<"2"<<endl;
        }
        else if(BMI >= 25 && BMI <= 29){
            cout<<"3"<<endl;
        }
        else {
            cout<<"4"<<endl;
        }


    }
    return 0;
}
