 Replace Elements with Greatest Element on Right Side.cpp


class Solution {
public:
    vector<int> replaceElements(vector<int>& arr) {
        int n=arr.size();
        int max = arr[n-1];
        arr[n-1] = -1;
        for(int i=n-2;i>=0;i--){
            int t = arr[i];
            arr[i]=max;
            if(t>max)
                max= t;
        }
        return arr;
        
    }
};
