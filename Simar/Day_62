#include <bits/stdc++.h>
using namespace std;

	float round2(float var)
   {
    float value = (int)(var * 100 + .5);
    return (float)value / 100.00;
   }
 

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    float k1,k2,k3,v;
	    cin>>k1>>k2>>k3>>v;
	    v= k1 * k2 * k3 * v;
	    float t  = (float)100.00/v;
	    t = round2(t);
	    t=t*100;
	    if(t>=958)
	    cout<<"NO"<<endl;
	    else
	    cout<<"YES"<<endl;
	}
	return 0;
}
