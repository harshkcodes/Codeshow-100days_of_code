#include <iostream>
using namespace std;
int main()

{
    int n,d,sum;
    cout<<"Enter number to be checked\n";
    cin>>n;
    int m=n;
    sum=0;
    while(n!=0)
    {
        d=n%10;
        sum=sum+(d*d*d);
        n=n/10;
    }
    if (sum==m)
    cout<<"\n Given number is Armstrong Number";
    else cout<<"Given number is not an Armstrong number";
    return 0;
}
