#include <iostream>
#include <string>
using namespace std;
int main()
{
    int n;
    cin>>n;
    string a;
    cin >> a;
    for (int d=0;d<n;d++){
        int ans=0;
        for (int i=0;i<=n-d-1;i++){
            if(a[i]=='1' && a[i]==a[i+d]){
                ans++;
            }
        }
        cout<<ans<<" ";
    }
    
}  
