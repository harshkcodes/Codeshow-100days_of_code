#include <iostream>
using namespace std;

int main()
{
	long long int t;
	cin >> t;
	while(t--)
	{
		long long int n, k, sum = 0;
		cin >> n >> k;
		long long int a[n];
		for(long long int i=0; i<n; i++)
		{
			cin >> a[i];
			sum = sum+a[i];
		}

		if(sum%k==0)
			cout << "0" << "\n";
		else
			cout << "1" << "\n";
	}
	return 0;
}
