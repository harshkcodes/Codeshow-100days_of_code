#include<bits/stdc++.h>
#define int long long int 
using namespace std;

signed main()
{
int t;
cin>>t;
while(t--)
{
int n,k;
cin>>n>>k;
int a[k];
for(int i=0;i<k;i++)
{
  cin>>a[i];
}

for(int i=0;i<n;i++)
{  int sum =0;
   string s;
   cin>>s;
  
   for(int m =0;m<k;m++)
   {
        int p = s[m] -'0';
       sum =  sum + (a[m] * p) ;
   }
   cout<<sum<<endl;
}
}
}
