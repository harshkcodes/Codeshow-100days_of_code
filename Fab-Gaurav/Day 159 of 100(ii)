#include<bits/stdc++.h>
using namespace std;

int main(){
	int t;
	cin>>t;
	while(t--)
	{
	    string s;
	    cin>>s;
	    int check = 0;
	    int l = s.length();
	    int i=0;
	    for(int i=0;i<l;i=i+2)
	    {
	        if(s[i]==s[i+1])
	        {
	             check=1;
	             break;
	        }	        
	    }
	    if(check==0)
	    {
	        cout<<"yes"<<endl;
	    }
	    else
	    {
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
