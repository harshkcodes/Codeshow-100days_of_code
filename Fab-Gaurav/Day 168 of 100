#include <bits/stdc++.h>
using namespace std;

int main(int argc, const char * argv[]){
    int k , n;
    cin>>n;
    cin>>k;
    
    int A[n];
    for(int i=0;i<n;i++)
    {
        cin>>A[i];
    }
    int count=0;
    
    for(int i=0;i<n;i++)
    {
        if(A[i]%k==0)
            count++;
    }
    cout<<count;
}
