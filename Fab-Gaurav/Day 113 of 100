#include<bits/stdc++.h>
using namespace std;
int main()
{
    int T;
    cin>>T;
    while (T--)    
    {
        int h=0,m=0,M;
        cin>>m>>h;
        h=h*h;
        M=m/h;
        if(M>=30)
        {
            cout<<"4"<<endl;
        }
        else if(M>=25 && M<=29)
        {
            cout<<"3"<<endl;
        }
        else if(M>=19 && M<=24)
        {
            cout<<"2"<<endl;
        }
        else
        {
            cout<<"1"<<endl;
        }
    }
    return(0);
}
