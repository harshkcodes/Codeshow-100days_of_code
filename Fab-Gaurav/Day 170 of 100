#include <bits/stdc++.h>
using namespace std;

int main(){
    unsigned int T, N, count = 0;
    scanf("%d", &T);
    for (int i = 0; i < T; i++)
    {
        scanf("%d", &N);
        for (int j = 1; j <= N; j++)
        {
            if (N % j <= (N / 2))
            {
                count++;
            }
        }
        printf("%d\n", count);
        count = 0;
    }
    return 0;
}
