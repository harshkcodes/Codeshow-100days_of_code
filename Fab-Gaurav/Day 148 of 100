#include <bits/stdc++.h>
using namespace std;
int  main(){
   cout<<"\nPattern 1"<<endl;
   for (int i = 1; i <= 4; ++i) {
      for (int j = 1; j <= i; ++j) {
         cout<<"*";
      }
      cout<<endl;
   }
   
   cout<<"\n\n";
   cout<<"Pattern 2"<<endl;
   for (int i = 0; i <= 3; ++i) {
      for (int j = 0; j <= i; ++j) {
         cout<<j<<" ";
      }
      cout<<endl;
   }
   return 0;
}
