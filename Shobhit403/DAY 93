#include <bits/stdc++.h>
using namespace std;
int main() 
{
    int t;
    cin>>t;
    while(t--)
    {
       int a,b,c,d;
       cin >> a >> b >> c >> d;
       int d1=abs(a-b);
       int d2=abs(c-d);
       if(a==b||(c!=d&&d1%d2==0))
       {
           cout<<"YES"<<endl;
       }
       else
       cout<<"NO"<<endl;      
    }
	return 0;
}
