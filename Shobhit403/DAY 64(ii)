#include <bits/stdc++.h>
using namespace std;
int main() 
{
	int t;
	cin>>t;
	while(t--)
  {
	    for(int i=1;i<1000;i++)
      {
	        int n;
	        cout<<i*i<<endl;
	        cout.flush();
	        cin>>n;
	              if(n==1)
                {
	            break;
	        }
}
  cout.flush();
	    }
	return 0;
}
