#include <iostream>
using namespace std;
int main()
{	
	int t;
	cin>>t;
	while(t--)
  {
	    string str;
	    cin>>str;
	    int p=0,c=0,m=0;
	    for(int i=0;i<3;i++)
      {
	        if(str[i]=='P')
          {
	            p=1;
	        }
	        if(str[i]=='C')
          {
	            c=1;
	        }
	        if(str[i]=='M')
          {
	            m=1;
	        }
	    }
	    if(p==1&&c==1&&m==1)
      {
	        cout<<"YES"<<"\n";
	    }
      else
      {
	        cout<<"NO"<<"\n";
	    }
	}
	return 0;
}
