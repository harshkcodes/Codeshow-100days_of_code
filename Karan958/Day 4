DO WHILE LOOP

#include <stdio.h>
int main()
{
    int i,j=0;
    printf("Enter a number:");
    scanf("%d",&i);
    do{
        printf("%d", j);
        j++;

    }while(j<=i);
    return 0;
}

WHILE LOOP

#include <stdio.h>
int main()
{
    int i=0;
    while(i<=30)
    {
        printf("%d \n",i);
        i++;
    }
    return 0;
}

FOR LOOP

#include <stdio.h>
int main()
{
 int i,j;
 for(i=0,j=0;i<5,j<5;i++,j++)   
 {
     printf("%d %d \n", i,j);
 }
    return 0;
}

